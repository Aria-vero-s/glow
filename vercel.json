{
	"version": 2,
	"builds": [
	  {
		"src": "myproject/wsgi.py",
		"use": "@vercel/python",
		"config": {
		  "runtime": "python3.9"
		}
	  },
	  {
		"src": "manage.py",
		"use": "@vercel/python",
		"config": {
		  "command": "python manage.py collectstatic --noinput"
		}
	  }
	],
	"routes": [
	  {
		"src": "/static/(.*)",
		"dest": "/staticfiles/$1"
	  },
	  {
		"src": "/(.*)",
		"dest": "myproject/wsgi.py"
	  }
	]
  }
